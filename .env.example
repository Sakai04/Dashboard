# Database Configuration (Existing)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# PostgreSQL Configuration (Existing)
POSTGRES_USER=dashboard_user
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=dashboard_db

# PgAdmin Configuration (Existing)
PGADMIN_DEFAULT_EMAIL=admin@dashboard.com
PGADMIN_DEFAULT_PASSWORD=admin_password

# ===========================================
# GitHub to Notion Tracker Configuration
# ===========================================

# Notion API Configuration
NOTION_API_TOKEN=secret_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
NOTION_DATABASE_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# GitHub API Configuration
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_WEBHOOK_SECRET=your-strong-webhook-secret-minimum-20-characters
GITHUB_REPOSITORY=Sakai04/Dashboard
GITHUB_OWNER=Sakai04

# n8n Webhook Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/github-commits
N8N_EXECUTION_WEBHOOK_URL=https://your-n8n-instance.com/webhook
N8N_HOST=your-n8n-instance.com
N8N_PORT=5678
N8N_PROTOCOL=https

# Dashboard Project Specific Configuration
PROJECT_TYPE=fastapi
PROJECT_LANGUAGE=python
PROJECT_FRAMEWORK=fastapi
PROJECT_VERSION=1.0.0
CI_CD_PLATFORM=jenkins

# Tracking and Monitoring Configuration
ENABLE_PRIORITY_DETECTION=true
ENABLE_SECURITY_ALERTS=true
ENABLE_DEPENDENCY_TRACKING=true
ENABLE_CICD_MONITORING=true
ENABLE_API_CHANGE_TRACKING=true
ENABLE_DATABASE_CHANGE_TRACKING=true

# File Priority Configuration
HIGH_PRIORITY_FILES=requirements.txt,Dockerfile,docker-compose.yml,Jenkinsfile,app/main.py
MEDIUM_PRIORITY_FILES=app/routers,app/models,app/schemas,app/crud
LOW_PRIORITY_FILES=README.md,docs,tests

# Security Monitoring
SECURITY_FILES=.env,.env.example,requirements.txt,Dockerfile,docker-compose.yml
SECURITY_KEYWORDS=password,secret,token,key,credential,auth
SECURITY_ALERT_THRESHOLD=1

# API Monitoring
API_RELATED_PATHS=app/routers,app/main.py,app/schemas
API_CHANGE_NOTIFICATION=true
API_DOCUMENTATION_AUTO_UPDATE=false

# Database Monitoring
DATABASE_RELATED_PATHS=app/models,app/database.py,app/crud
DATABASE_MIGRATION_DETECTION=true
SCHEMA_CHANGE_ALERT=true

# CI/CD Integration
JENKINS_WEBHOOK_URL=https://your-jenkins-instance.com/webhook
JENKINS_BUILD_NOTIFICATION=true
DEPLOYMENT_TRACKING=true
BUILD_STATUS_UPDATE=true

# Notification Configuration (Optional)
# Slack Integration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/T00000000/B00000000/XXXXXXXXXXXXXXXXXXXXXXXX
SLACK_CHANNEL=#development
SLACK_USERNAME=Dashboard-Bot
SLACK_ENABLE_SECURITY_ALERTS=true
SLACK_ENABLE_BUILD_NOTIFICATIONS=true

# Discord Integration (Optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/000000000000000000/xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
DISCORD_ENABLE_NOTIFICATIONS=false

# Email Notifications (Optional)
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_RECIPIENTS=developer1@company.com,developer2@company.com
EMAIL_ENABLE_SECURITY_ALERTS=true

# Logging and Debugging
LOG_LEVEL=INFO
LOG_FILE=logs/github-notion-tracker.log
DEBUG_MODE=false
VERBOSE_LOGGING=false

# Rate Limiting
GITHUB_API_RATE_LIMIT=5000
NOTION_API_RATE_LIMIT=3
WEBHOOK_PROCESSING_DELAY=1000

# Performance Optimization
BATCH_PROCESSING=true
MAX_COMMITS_PER_BATCH=50
PROCESSING_TIMEOUT=30000
RETRY_FAILED_REQUESTS=3
RETRY_DELAY=5000

# Advanced Features
COMMIT_ANALYSIS_DEPTH=detailed
INCLUDE_COMMIT_DIFF=true
TRACK_FILE_CHANGES=true
ANALYZE_CODE_COMPLEXITY=false
GENERATE_COMMIT_INSIGHTS=true

# Backup and Recovery
BACKUP_NOTION_DATA=true
BACKUP_INTERVAL=daily
BACKUP_RETENTION_DAYS=30
BACKUP_STORAGE_PATH=backups/

# Health Monitoring
HEALTH_CHECK_INTERVAL=300000
WEBHOOK_TIMEOUT=30000
API_RESPONSE_TIMEOUT=10000
MONITOR_SYSTEM_RESOURCES=true

# Feature Toggles
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_MACHINE_LEARNING_INSIGHTS=false

# Timezone and Localization
TIMEZONE=Asia/Seoul
DATE_FORMAT=YYYY-MM-DD HH:mm:ss
LANGUAGE=ko-KR